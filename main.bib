	Title = {The Hidden Geometry of Complex, Network-Driven Contagion Phenomena},
	Volume = {342},
	Year = {2013},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://science.sciencemag.org/content/342/6164/1337},
